<!doctype html>
<html lang="en">
  <head>
        <title>Pages - Persons</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= javascript_pack_tag 'reports' %> 

  </head>
  <body>
  <%= yield %>
    <!-- Display a message if JS has been disabled on the browser. -->
    <noscript>
      If you're seeing this message, that means <strong>JavaScript has been disabled on your browser</strong>, please     <strong>enable JS</strong> to make this app work.
    </noscript>

    <!-- The app hooks into this div -->
    <%= link_to '<button type="button" class="logout" >Log Out</button>'.html_safe, '/signout', id: 'logout', style: "width: 10%;"  %>
    <div id="app">
    <%= javascript_packs_with_chunks_tag 'reports' %>
    </div>
  </body>
</html>
